function fechax( dias ) {
    //fecha menos dias 
    var now = new Date();
    now.setDate(now.getDate() - dias);
    var month = (now.getMonth() + 1);               
    var day = now.getDate();
    if (month < 10) 
        month = "0" + month;
    if (day < 10) 
        day = "0" + day;
    var today = now.getFullYear() + '-' + month + '-' + day;

    return today
}













                                        <div class="card-block">
                                            <h6 class="mb-4">Recaudacion Normal</h6>
                                            <div class="row d-flex align-items-center">
                                                <div class="col-9">
                                                    <h3 class="f-w-300 d-flex align-items-center  m-b-0"><i class="feather icon-arrow-up text-c-green f-30 m-r-10"></i><% if (abonos) { var a = JSON.parse(abonos) ; var abo = a[0]; %>   <%=  abo.RNORMAL %><% } %></h3>
                                                </div>
                                                <div class="col-3 text-right">
                                                    <p class="m-b-0">100%</p>
                                                </div>
                                            </div>
                                            <div class="progress m-t-30" style="height: 7px;">
                                                <div class="progress-bar progress-c-theme2" role="progressbar" style="width: 100%;" aria-valuenow="35" aria-valuemin="0" aria-valuemax="100"></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!--[ Monthly  sales section ] end-->
                                <!--[ year  sales section ] starts-->
                                <div class="col-md-12 col-xl-4">
                                    <div class="card yearly-sales">
                                        <div class="card-block">
                                            <h6 class="mb-4">Recaudacion Castigo</h6>
                                            <div class="row d-flex align-items-center">
                                                <div class="col-9">
                                                    <h3 class="f-w-300 d-flex align-items-center  m-b-0"><i class="feather icon-arrow-up text-c-green f-30 m-r-10"></i><% if (abonos) { var a = JSON.parse(abonos) ; var abo = a[0]; %>   <%=  abo.RCASTIGO %><% } %></h3>
                                                </div>
                                                <div class="col-3 text-right">
                                                    <p class="m-b-0">100%</p>
                                                </div>
                                            </div>
                                            <div class="progress m-t-30" style="height: 7px;">
                                                <div class="progress-bar progress-c-theme" role="progressbar" style="width: 100%;" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100"></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!--[ year  sales section ] end-->
                                <!--[ Recent Users ] start-->
                                <div class="col-xl-8 col-md-6">
                                    <div class="card Recent-Users">
                                        <div class="card-header">
                                            <h5>Ultimo Mes Recaudación</h5>
                                        </div>
                                        <div class="card-block px-0 py-3 table-scrollable">
                                            <table class="table table-striped">
                                                <thead class="thead-dark">
                                                  <tr>
                                                    <th scope="col">FECHA</th>
                                                    <th scope="col">NORMAL</th>
                                                    <th scope="col">CASTIGO</th>
                                                    <th scope="col">TOTAL</th>
                                                  </tr>
                                                </thead>
                                                <tbody>
                                                <%  
                                               
                                                  if(Object.keys(resultado).length > 0){
                                                    var stringify = JSON.parse(resultado)  

                                                 for (var i = 0; i < 31; i++) {   

                                                 if (typeof stringify[0][i] != 'undefined'){ %> 
                                                    <tr>
                                                       <td>  <%=stringify[0][i]['FECHA'] %>  </td>  
                                                       <td>  <%=stringify[0][i]['NORMAL'] %>  </td>  
                                                       <td>  <%=stringify[0][i]['CASTIGO']%> </td>  
                                                       <td>  <%=stringify[0][i]['TOTAL']%> </td> 
                                                    </tr>
                                                       <%      }else{    %>

                                                        <%  break  %>
                                                           <td>  SIN DATOS </td>
                                                <%     }  %>
                                                        
                                                        
                                                <%     }  %>
                                                <%    } %>
                                         
                                            
                                                </tbody>
                                            </table>
                                                
                                        </div>
                                    </div>
                                </div>
                                <!--[ Recent Users ] end-->

                                <!-- [ statistics year chart ] start -->
                                <div class="col-xl-4 col-md-6">
                                    <div class="card card-event">
                                        <div class="card-block">
                                            <div class="row align-items-center justify-content-center">
                                                <div class="col">
                                                    <h5 class="m-0">Recaudacion  Hoy</h5>
                                                </div>
                                                <div class="col-auto">
                                                    <label class="label theme-bg2 text-white f-14 f-w-400 float-right">100%</label>
                                                </div>
                                            </div>
                                            <h2 class="mt-3 f-w-300"><% if (abonos) { var a = JSON.parse(abonos) ; var abo = a[0]; %>   <%=  abo.RHOY %><% } %><sub class="text-muted f-14">Abonos</sub></h2>
                                          <!--  <h6 class="text-muted mt-4 mb-0">Recaudado el dia de hoy </h6>
                                            <i class="fab fa-angellist text-c-purple f-50"></i>-->
                                        </div>
                                    </div>
                                    <div class="card">
                                        <div class="card-block border-bottom">
                                            <div class="row d-flex align-items-center">
                                                <div class="col-auto">
                                                    <i class="feather icon-zap f-30 text-c-green"></i>
                                                </div>
                                                <div class="col">
                                                    <h3 class="f-w-300"><% if (abonos) { var a = JSON.parse(abonos) ; var abo = a[0]; %>   <%=  abo.RNORMALHOY %><% } %></h3>
                                                    <span class="d-block text-uppercase">Recaudacion Normal hoy</span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="card-block">
                                            <div class="row d-flex align-items-center">
                                                <div class="col-auto">
                                                    <i class="feather icon-zap f-30 text-c-blue"></i>
                                                </div>
                                                <div class="col">
                                                    <h3 class="f-w-300"><% if (abonos) { var a = JSON.parse(abonos) ; var abo = a[0]; %>   <%=  abo.RCASTIGOHOY %><% } %></h3>
                                                    <span class="d-block text-uppercase">Recaudacion Castigo Hoy</span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                       
                                <!-- [ statistics year chart ] end -->
                           </div>
                            <!-- [ Main Content ] end -->
                        </div>
